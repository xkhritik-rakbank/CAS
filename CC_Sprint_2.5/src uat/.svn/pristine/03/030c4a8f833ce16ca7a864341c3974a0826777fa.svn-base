	public void submitFormStarted(FormEvent pEvent) throws ValidatorException {
		FormReference formObject = FormContext.getCurrentInstance().getFormReference();

		try{
		DigitalOnBoarding.mLogger.info( "Inside PL PROCESS submitFormStarted()" + pEvent.getSource());
		//below code commented by nikhil 7/12/17
		//formObject.setNGValue("Decision", formObject.getNGValue("cmplx_DEC_Decision"));
		formObject.setNGValue("OV_dec", formObject.getNGValue("cmplx_DEC_Decision"));
		DigitalOnBoarding.mLogger.info( "Inside PL OV_dec submitFormStarted()" +formObject.getNGValue("OV_dec"));
			 
		DigitalOnBoarding.mLogger.info( "Inside SFS OV WI: "+formObject.getNGValue("cmplx_DEC_Decision"));
		//modified by kashay on 4/1/17
		if("Approve".equalsIgnoreCase(formObject.getNGValue("cmplx_DEC_Decision"))){
			DigitalOnBoarding.mLogger.info( "Inside Approve SFS OV WI: "+formObject.getWFWorkitemName());
			formObject.setNGValue("ORIGINAL_VALIDATION", "Yes");
			DigitalOnBoarding.mLogger.info( "Value of ORIGINAL_VALIDATION is : "+formObject.getNGValue("ORIGINAL_VALIDATION"));
			formObject.setNGValue("q_Hold2", 1);
		}
		else if("Reject".equalsIgnoreCase(formObject.getNGValue("cmplx_DEC_Decision"))){
			DigitalOnBoarding.mLogger.info( "Inside Reject SFS OV WI: "+formObject.getWFWorkitemName());
			formObject.setNGValue("ORIGINAL_VALIDATION", "No");
			formObject.setNGValue("q_Hold2", 1);
			//Deepak Changes done for PCAS-2926
			if("Re-Initiate".equalsIgnoreCase(formObject.getNGValue("Reject_DeC"))){
				formObject.setNGValue("Reject_DeC","");
			}
		}
		
		DigitalOnBoarding.mLogger.info( "Value of ORIGINAL_VALIDATION is : "+formObject.getNGValue("ORIGINAL_VALIDATION"));
		
		}catch(Exception ex){
			DigitalOnBoarding.mLogger.info( "Inside Exception submitFormstarted");
		}
		finally{
			//app OV validation for document section
			OriginalDocs(); 
			saveIndecisionGrid();
			LoadReferGrid();
			String squeryrefer = "select referTo from NG_RLOS_GR_DECISION where workstepName like '%orignal%' and dec_wi_name= '" + formObject.getWFWorkitemName() + "' order by dateLastChanged desc ";
			List<List<String>> FCURefer = formObject.getNGDataFromDataCache(squeryrefer);
			//CreditCard.mLogger.info("RLOS COMMON"+ " iNSIDE prev_loan_dbr+ " + squeryloan);
			String referto="";
			if (FCURefer != null && FCURefer.size() > 0) {
				referto = FCURefer.get(0).get(0);
				}
			if("FPU".equalsIgnoreCase(referto)){		
				formObject.setNGValue("RefFrmOV", "Y");
			}
			
		}
	}


